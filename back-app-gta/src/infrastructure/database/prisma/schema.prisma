generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role {
  ADMIN
  USER
  MANAGER
  RH
}

model User {
  id       String @id @default(cuid())
  email    String @unique
  password String
  role     Role   @default(USER)
}